{
  "expo": {
    "name": "CCT Dispatcher",
    "slug": "cct-dispatcher-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "cctdispatcher",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5fbfc0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.compassionatecare.dispatcher",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "CCT Dispatcher needs access to your location to help manage trip routes and driver assignments."
      },
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyDylwCsypHOs6T9e-JnTA7AoqOMrc3hbhE"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5fbfc0"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.compassionatecare.dispatcher",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDylwCsypHOs6T9e-JnTA7AoqOMrc3hbhE"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification.png",
          "color": "#5fbfc0"
        }
      ],
      "expo-font"
    ],
    "owner": "jass_khinda"
  }
}
