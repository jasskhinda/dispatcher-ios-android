{
  "expo": {
    "name": "CCT Dispatcher",
    "slug": "cct-dispatcher-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons.png",
    "userInterfaceStyle": "light",
    "scheme": "cctdispatcher",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5fbfc0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.compassionatecare.dispatcher",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "CCT Dispatcher needs access to your location to help manage trip routes and driver assignments."
      },
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyDylwCsypHOs6T9e-JnTA7AoqOMrc3hbhE"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/androidicon.png",
        "backgroundColor": "#5fbfc0"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.compassionatecare.dispatcher",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDylwCsypHOs6T9e-JnTA7AoqOMrc3hbhE"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#5fbfc0"
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      "expo-font"
    ],
    "owner": "jass_khinda",
    "extra": {
      "oneSignalAppId": "ff9262e5-fe8a-4b14-827f-d67232a4c688",
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.compassionatecare.dispatcher.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.compassionatecare.dispatcher.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "24b8d1bf-2bd7-49bd-b76d-fa3602345566"
      }
    }
  }
}
